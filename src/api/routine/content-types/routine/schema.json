{
  "kind": "collectionType",
  "collectionName": "routines",
  "info": {
    "singularName": "routine",
    "pluralName": "routines",
    "displayName": "Routine",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "steps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::routine-step.routine-step",
      "mappedBy": "routine"
    },
    "reminderDay": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "reminderWeekday": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "reminderFrequency": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "daily",
        "monthly",
        "weekly"
      ]
    },
    "executions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::routine-execution.routine-execution",
      "mappedBy": "routine"
    },
    "reminderHour": {
      "pluginOptions": {
        "i18n": {
          "localized": true
